@page

<div class="header">

    <h1 class="font-effect-3d">Sticky Notes</h1>
</div>
<div class="strong">
    <div class="create">
        <h1><a href="/Board">Create a new board!</h1></a>
        <form action="/Board" method="post">
            <!-- Create New Board -->
            <input type="submit" name ="boardId" value="Create" id="createButton">
        </form>
    </div>
    <div class="join">
        <h1>Join a friend's board!</h1>
        <form>
            Code
            <input type="text" name="code" placeholder="Enter 9 digit code here">
            <br>
            <input type="submit" value="Join" id="joinButton">
        </form>
    </div>
</div>



